{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "react-snap",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json"
  },
  "reactSnap": {
    "source": "dist",
    "include": [
      "/",
      "/services",
      "/pricing",
      "/portfolio",
      "/about",
      "/contact",
      "/consultation",
      "/terms",
      "/offers"
    ],
    "puppeteerArgs": ["--no-sandbox", "--disable-setuid-sandbox"],
    "crawl": false
  }
}
